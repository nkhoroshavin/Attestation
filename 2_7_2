#include <iostream>
using namespace std;
int main()
{
  int n;

  cout << "Введите длину стороны квадрата: "; //Пояснение для пользователя
  cin >> n; //Запрос на ввод числа пользователем

  for (int i = 1; i <= n; i++) //Цикл генерирует n количество строк
  {
    for (int j = 1; j <= n; j++) //Цикл генерирует строку длиной n звездочек
    {
      cout << " *"; //Вывод звездочки
    }
    cout << "\n"; //Переход на новую строку
  }

  return 0;
}
