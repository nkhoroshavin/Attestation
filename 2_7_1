#include <iostream>
#include <cmath>
using namespace std;
int main()
{
  int s,n;
  cout << "Введите площадь квадрата: ";//Пояснение для пользователя
  cin >> s; //Запрос на ввод числа пользователем
  n = sqrt(s); //Извлечение квадрата из введенного числа

  for (int i = 1; i <= n; i++) //Цикл генерирует n количество строк
  {
    for (int j = 1; j <= n; j++) //Цикл генерирует строку длиной n звездочек
    {
      cout << " *"; //Вывод звездочки
    }
    cout << "\n"; //Переход на новую строку
  }

  return 0;
}
